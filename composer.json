{
    "name": "abenevaut/laravel-email-reset",
    "description": "Laravel package for email reset.",
    "keywords": [
        "email",
        "laravel"
    ],
    "license": "GPL-3.0-only",
    "homepage": "https://github.com/abenevaut/laravel-email-reset",
    "support": {
        "issues": "https://github.com/abenevaut/laravel-email-reset/issues",
        "source": "https://github.com/abenevaut/laravel-email-reset"
    },
    "authors": [
        {
            "name": "yaquawa",
            "email": "yaquawa@gmail.com"
        },
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/notifications": "^6.0|^7.0|^8.0",
        "yaquawa/laravel-token-repository": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/events": "^6.0|^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "abenevaut\\Laravel\\EmailReset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "abenevaut\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "abenevaut\\Laravel\\EmailReset\\EmailResetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpcs --standard=PSR12 ./src"
        ]
    }
}
